{
  "name": "react-responsive-table",
  "version": "1.0.6",
  "description": "A react component for building really responsive table",
  "main": "dist/index.js",
  "scripts": {
    "coverage": "nyc npm test && nyc report --require babel-core/register --require babel-polyfill mocha \"src/**/*.test.js\" --reporter=text-lcov > coverage.lcov  && codecov",
    "test": "mocha \"src/**/*.test.js\" --compilers js:babel-register",
    "eslint": "eslint src",
    "lint": "npm run eslint",
    "build": "webpack --progress -p --config webpack.config.dist.js",
    "clean": "rm -rf dist/*",
    "builddemo": "webpack --progress -p --config webpack.config.example.js",
    "publishdemo": "gh-pages -d dist/examples",
    "bump-patch": "npm test && npm version patch && npm run build && npm publish && git push && git push --tags && npm run builddemo && npm run publishdemo",
    "start": "webpack-dev-server -d --colors --inline --hot --port 3006 --host 0.0.0.0 --config webpack.config.example.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jtassin/react-responsive-table.git"
  },
  "nyc": {
    "exclude": [
      "**/*.test.js",
      "node_modules"
    ]
  },
  "keywords": [
    "table",
    "responsive",
    "react",
    "component",
    "flexbox"
  ],
  "author": "Julien TASSIN",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jtassin/react-responsive-table/issues"
  },
  "homepage": "https://github.com/jtassin/react-responsive-table#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "gh-pages": "^0.11.0",
    "html-webpack-plugin": "^2.22.0",
    "mocha": "^3.0.0",
    "nyc": "^7.1.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react": "^15.3.0",
    "react-addons-pure-render-mixin": "^15.3.0"
  }
}
